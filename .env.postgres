# PostgreSQL environment configuration
# Use with: docker compose --env-file .env.postgres --profile postgres up
# OR: Copy to .env and use: docker compose --profile postgres up
COMPOSE_PROFILES=postgres
AGOR_DB_DIALECT=postgresql
DATABASE_URL=postgresql://agor:agor_dev_secret@postgres:5432/agor
SEED=true
DAEMON_PORT=4091
UI_PORT=6091
